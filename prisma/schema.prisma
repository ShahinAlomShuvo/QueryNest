generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
  previewFeatures = ["pgVector"] 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Document {
  id        Int      @id @default(autoincrement())
  content   String
  vector  Vector   @vector(dim: 768)
}